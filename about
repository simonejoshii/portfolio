                        <div class="hero-about__text-wrap">
                            <div class="hero-title-wrap main-padding about">
                                <div class="title-wrap mr-12--mob">
                                    <h1 style="opacity:0;-webkit-transform:translate3d(0, 11vw, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-moz-transform:translate3d(0, 11vw, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);-ms-transform:translate3d(0, 11vw, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0);transform:translate3d(0, 11vw, 0) scale3d(1, 1, 1) rotateX(0) rotateY(0) rotateZ(0) skew(0, 0)" class="title-h1 text-reveal _01 text-black">Simone Joshi,</h1>
                                </div>
                                <div class="title-wrap---line space-between about">
                                    <div class="flex-h align-center grow mr-32">
                                      <div class="title-wrap noshrink">
                                        <h1 class= "title-h1 text-reveal _01 text-black">
                                          <span id="meFactSpan">cuts her own bangs.</span>
                                        </h1>
                                        <script>
                                            // Define an array of facts about you
                                            const facts = [
                                              "cuts her own bangs.",
                                              "chicken biryani enthusiast.",
                                              "hugs trees.",
                                              "will walk everywhere.",
                                              "likes to smile a lot.",
                                              "has a turtleneck obsession.",
                                              "loves sketching faces.",
                                              "can never finish her ice-cream.",
                                              "mum\'s closet raider.",
                                              "never says no to an adventure.",
                                              "wants to run a marathon one day.",
                                              "can\'t swim but still dives in.",
                                              "keeps a sketchbook handy.",
                                              "prefers sunrises.",
                                              "unapologetically tone-deaf.",
                                              "organises her files."
                                            ];

                                            // Function to shuffle the facts array
                                            function shuffleArray(array) {
                                                for (let i = array.length - 1; i > 0; i--) {
                                                    const j = Math.floor(Math.random() * (i + 1)); // Random index
                                                    [array[i], array[j]] = [array[j], array[i]]; // Swap elements
                                                }
                                            }
                                         
                                            // Function to change the fact automatically
                                            function changeFact() {
                                              // Get the element where the fact should appear
                                              const factElement = document.getElementById('meFactSpan');

                                              // Shuffle the facts array for the first time
                                              shuffleArray(facts);
                                            
                                              // Set a counter to keep track of which fact is being shown
                                              let factIndex = 0;
                                          
                                              // Use setInterval to change the fact every 3 seconds (3000 ms)
                                              const intervalId = setInterval(() => {
                                                factElement.textContent = facts[factIndex]; // Update the fact content
                                                factIndex++; // Move to the next fact

                                                // If all facts have been displayed, reshuffle the array and reset the index
                                                if (factIndex === facts.length) {
                                                    shuffleArray(facts); // Reshuffle the facts
                                                    factIndex = 0; // Reset index to start from the beginning
                                                }
                                              }, 2000); // 2000 milliseconds = 2 seconds
                                            }
                                          
                                            // Call the function to start the automatic change
                                            changeFact();
                                        </script>
                                      </div>
                                    </div>
                                </div>
                            </div>
                        </div>
